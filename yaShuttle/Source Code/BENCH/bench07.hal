       SUBBIT(IVALUE) = BITVAR12_2$(IDEX1,IDEX2:11 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_2$(IDEX1,IDEX2:12 AT BIT_LOC);
     END;
     DO CASE BIT_CNT;
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:1 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:2 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:3 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:4 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:5 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:6 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:7 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:8 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:9 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:10 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:11 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_3$(IDEX1,IDEX2,IDEX3:12 AT BIT_LOC);
     END;
   END;
 CLOSE FETCH_BIT12;
