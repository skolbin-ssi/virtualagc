   DO CASE IARRAY;
     DO CASE BIT_CNT;
       SUBBIT(IVALUE) = BITVAR12_1$(IDEX1:1 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_1$(IDEX1:2 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_1$(IDEX1:3 AT BIT_LOC);
       SUBBIT(IVALUE) = BITVAR12_1$(IDEX1:4 AT BIT_LOC);
