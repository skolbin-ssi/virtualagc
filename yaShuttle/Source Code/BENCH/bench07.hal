 BITS: PROGRAM;
   REPLACE ITER_MAX BY "300";
   REPLACE STORAGE_MAX BY "850";
   REPLACE STORAGE_CRIT BY "384";
C
C VARIABLES FOR RANDOM NUMBER GENERATOR
C
  DECLARE RM INTEGER DOUBLE INITIAL(2**20);
  DECLARE FM SCALAR DOUBLE CONSTANT(2**20);
  DECLARE RA INTEGER DOUBLE CONSTANT(2**10 + 3);
  DECLARE RX INTEGER DOUBLE INITIAL(566387);
C
C LOCAL VARIABLES:
C
  DECLARE SCALAR DOUBLE, LIMIT1, LIMIT2;
  DECLARE BIT_TYPES ARRAY(6) CHARACTER(5) INITIAL(' 12/1',' 12/2',
    ' 12/3',' 16/1', ' 16/2', ' 16/3');
  DECLARE INTEGER DOUBLE, BIT_NDXD, ITER_NDX, FAIL_CTR INITIAL(0);
  DECLARE INTEGER DOUBLE, IVALUE, POWERS ARRAY(31) INITIAL(1.0,*);
  DECLARE INTEGER SINGLE, BIT_CNT, IDEX1, IDEX2, IDEX3,
    BIT_NDX, IARRAY, BIT_LOC;
  DECLARE INTEGER DOUBLE, ISUB, IARRAYD, BIT_LOCD,
    STORAGE_CNT INITIAL(0), STORAGE_NDX, REAL_IVALUE;
  DECLARE ARRAY(STORAGE_MAX) BIT(32) REMOTE,
    IVALUE_DATA INITIAL(BIN'0');
  DECLARE ARRAY(STORAGE_MAX) INTEGER DOUBLE REMOTE,
    IVALUE_ARR INITIAL(0);
  DECLARE BOOLEAN, SUCCESS_FLAG INITIAL(FALSE);
  DECLARE BITVAR12_1 ARRAY(64) BIT(12) INITIAL(BIN'0');
  DECLARE BITVAR12_2 ARRAY(8,8) BIT(12) INITIAL(BIN'0');
  DECLARE BITVAR12_3 ARRAY(4,4,4) BIT(12) INITIAL(BIN'0');
  DECLARE MSKVAR12_1 ARRAY(64) BIT(12) INITIAL(BIN'0');
  DECLARE MSKVAR12_2 ARRAY(8,8) BIT(12) INITIAL(BIN'0');
  DECLARE MSKVAR12_3 ARRAY(4,4,4) BIT(12) INITIAL(BIN'0');
  DECLARE BITVAR16_1 ARRAY(64) BIT(16) INITIAL(BIN'0');
  DECLARE BITVAR16_2 ARRAY(8,8) BIT(16) INITIAL(BIN'0');
  DECLARE BITVAR16_3 ARRAY(4,4,4) BIT(16) INITIAL(BIN'0');
  DECLARE MSKVAR16_1 ARRAY(64) BIT(16) INITIAL(BIN'0');
  DECLARE MSKVAR16_2 ARRAY(8,8) BIT(16) INITIAL(BIN'0');
  DECLARE MSKVAR16_3 ARRAY(4,4,4) BIT(16) INITIAL(BIN'0');
C
 I_RANDFUNC: PROCEDURE (LOWLIM, HIGHLIM) ASSIGN (OUTVAL);
  DECLARE SCALAR DOUBLE, LOWLIM, HIGHLIM;
  DECLARE INTEGER DOUBLE, OUTVAL;
  DECLARE SCALAR DOUBLE, TEMP, TEMP1;
  RX = MOD(RA RX,RM);
  TEMP = RX/FM;
  TEMP1 = (1 - TEMP) LOWLIM + TEMP (HIGHLIM + .999999);
  OUTVAL = FLOOR(TEMP1);
  CLOSE I_RANDFUNC;
C
 STUFF_BIT12: PROCEDURE;
   DO CASE IARRAY;
     DO;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         IF MSKVAR12_1$(IDEX1:BIT_LOC + IX - 1) = BIN'1'
           THEN RETURN;
       END;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         MSKVAR12_1$(IDEX1:BIT_LOC + IX - 1) = BIN'1';
       END;
       SUCCESS_FLAG = TRUE;
       DO CASE BIT_CNT;
         BITVAR12_1$(IDEX1:1 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:2 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:3 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:4 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:5 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:6 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:7 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:8 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:9 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:10 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:11 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_1$(IDEX1:12 AT BIT_LOC) = BIT(IVALUE);
       END;
     END;
     DO;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         IF MSKVAR12_2$(IDEX1,IDEX2:BIT_LOC + IX - 1) = BIN'1' THEN
           RETURN;
       END;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         MSKVAR12_2$(IDEX1,IDEX2:BIT_LOC + IX - 1) = BIN'1';
       END;
       SUCCESS_FLAG = TRUE;
       DO CASE BIT_CNT;
         BITVAR12_2$(IDEX1,IDEX2:1 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:2 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:3 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:4 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:5 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:6 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:7 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:8 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:9 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:10 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:11 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_2$(IDEX1,IDEX2:12 AT BIT_LOC) = BIT(IVALUE);
       END;
     END;
     DO;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         IF MSKVAR12_3$(IDEX1,IDEX2,IDEX3:BIT_LOC + IX - 1) = BIN'1'
           THEN RETURN;
       END;
       DO FOR TEMPORARY IX = 1 TO BIT_CNT;
         MSKVAR12_3$(IDEX1,IDEX2,IDEX3:BIT_LOC + IX - 1) = BIN'1';
       END;
       SUCCESS_FLAG = TRUE;
       DO CASE BIT_CNT;
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:1 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:2 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:3 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:4 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:5 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:6 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:7 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:8 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:9 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:10 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:11 AT BIT_LOC) = BIT(IVALUE);
         BITVAR12_3$(IDEX1,IDEX2,IDEX3:12 AT BIT_LOC) = BIT(IVALUE);
       END;
     END;
   END;
 CLOSE STUFF_BIT12;
