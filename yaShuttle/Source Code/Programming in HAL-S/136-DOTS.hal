C/ From PDF p. 136 of "Programming in HAL/S".

  DOTS:
  PROCEDURE(A1, A2) ASSIGN(RESULT);
     DECLARE ARRAY(10) VECTOR(3),
                A1, A2;
     DECLARE RESULT MATRIX(10, 10);
     DO FOR TEMPORARY I = 1 TO 10;
        DO FOR TEMPORARY J = 1 TO 10;
E                     -      -
M         RESULT    = A1   . A2  ;
S               I,J     I:     J:

        END;
     END;
  CLOSE DOTS;
  