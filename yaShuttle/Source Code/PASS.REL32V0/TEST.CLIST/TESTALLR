00010000/********************************************************************/
00020000/*------------------------------------------------------------------*/
00030000/*                                                                  */
00040000/*   NAME:     TESTALL                                              */
00050000/*                                                                  */
00060000/*------------------------------------------------------------------*/
00070000/*                                                                  */
00080000/*   FUNCTION: DRIVER CLIST TO BATCH CLIST T0 SUBMIT BATCH JOBS TO  */
00090000/*             EXECUTE THE COMMAND TO RUN ALL FUNCTIONAL TESTS.     */
00091000/*             COMPOOL MUST BE RUN BEFORE TESTALL IS SUBMITTED.     */
00091100/*                                                                  */
00092000/*------------------------------------------------------------------*/
00093000/*                                                                  */
00094000/*   REVISION HISTORY:                                              */
00095000/*                                                                  */
00096000/*  DATE      PCR  NAME  DESCRIPTION                                */
00097000/*  --------  ---  ---   -----------------------------------------  */
00098000/*  09/10/02  730  LWW   BASELINED COMPILER TEST CLIST              */
00099000/*------------------------------------------------------------------*/
00100000/********************************************************************/
00110000
00120000PROC 0 CMD(T32V0)          +
00130000       OPT1(FIRSTRUN)      +
00130100       OPT2()              +
00140000       SYSTEM(PASS)
00150000
00160000GLOBAL RETCODE
00160100IF &OPT2. NE THEN SET &OPT2 = OPT2(&OPT2.)
00160200             ELSE SET &OPT2 =
00180001BATCH  JOB(A) COMMAND(&CMD) OPTION(ARRAY)   OPT1(&OPT1) &OPT2
