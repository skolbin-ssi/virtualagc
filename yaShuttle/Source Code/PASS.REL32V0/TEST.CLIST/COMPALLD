00010000/********************************************************************/
00020000/*------------------------------------------------------------------*/
00030000/*                                                                  */
00040000/*   NAME:     COMPALLD                                             */
00050000/*                                                                  */
00060000/*------------------------------------------------------------------*/
00070000/*                                                                  */
00080000/*   FUNCTION: DRIVER CLIST TO COMPALL TO COMPARE A TEST CATEGORY.  */
00091100/*                                                                  */
00091200/*------------------------------------------------------------------*/
00091300/*                                                                  */
00091400/*   REVISION HISTORY:                                              */
00091500/*                                                                  */
00091600/*  DATE      PCR  NAME  DESCRIPTION                                */
00091700/*  --------  ---  ---   -----------------------------------------  */
00091812/*  09/10/02  730  LWW   BASELINED FOR COMPILER TEST CLIST          */
00091900/*------------------------------------------------------------------*/
00092000/********************************************************************/
00093000
00094000PROC 1 CAT                 +
00094100       ID(NCAMTST)         +
00094212       REL(R32V0)            +
00094312       OLDREL(R31V01)       +
00094413       BFSOI(BFS305XX)     +
00094514       SRCDSN(NCAMTST.TESTLIB.&CAT..HAL)  +
00094600       OBJ                 +
00094700       NOOBJ               +
00094800       SIM                 +
00094900       REFRESH             +
00095000       LIST()              +
00095100       TRACE
00095200
00095300GLOBAL RETCODE
00095400IF &TRACE.  NE THEN +
00095500    CONTROL  LIST CONLIST
00095600IF &LIST. NE THEN SET &LIST = LIST(&LIST.)
00095700   ELSE SET  &LIST =
00095800
00095900COMPALL &CAT.            +
00096000        ID(&ID.)         +
00096100        REL(&REL.)       +
00096200        OLDREL(&OLDREL.) +
00096300        BFSOI(&BFSOI.)   +
00096400        SRCDSN(&SRCDSN.)   +
00096500        &REFRESH.        +
00096600        &OBJ.            +
00097000        &NOOBJ.          +
00097100        &SIM.            +
00097200        &LIST.           +
00098000        &TRACE.
