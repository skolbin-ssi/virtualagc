00200000
00210000PROC     1  GROUP                                               +
00220000            ID(NCAMTST)                                         +
00230010            SRCID(NCAMCM)                                       +
00240009            DSN(R32V0#D)                                        +
00250000            LIB(&ID..&DSN..&GROUP.)                             +
00260010            SRCDSN(&SRCID..TESTLIB.&GROUP..HAL)                 +
00270009            COMPVER(REL32V0)                                    +
00280000            RUNLVER(&COMPVER.)                                  +
00290010            COMPILER(&ID..PASS.&COMPVER..COMPILER)              +
00300010            RUNLIB(&ID..PASS.&RUNLVER..RUNLIB)                  +
00310000            SIMLIB(NCAMCM.TOOLS.PASS.SIMLIB)                    +
00320010            ERRORLIB(&ID..PASS.&COMPVER..ERRORLIB)              +
00330010            MONITOR(&ID..PASS.&COMPVER..MONITOR.LOAD(MONITOR))  +
00331010            ZCONLIB(&ID..PASS.&COMPVER..ZCONOBJ)                +
00340000            TRUNLIB()                                           +
00350010            TERRORLIB(&SRCID..TESTLIB.REL31V0.TERRLIB)          +
00360000            HALSDF2(&ID..&DSN..COMPOOL.SDF)                     +
00370000            HALLOAD2(&ID..&DSN..COMPOOL.LOAD)                   +
