       IF AM3$(I,J) ~= AM1$(I,J) + AM2$(I,J) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed addition of MATRIX', BB) ASSIGN(ERRORS, TOTAL);

    AI3 = AI1 - AI2;
