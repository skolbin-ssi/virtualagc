    		C FUNCTION CHARACTER(10), B FUNCTION BIT(5),
    		I INTEGER, S SCALAR, V VECTOR, M MATRIX,
    		A CHARACTER(10), D BIT(5);
    DECLARE 	MM MATRIX INITIAL(1, 2, 3, 4, 5, 6, 7, 8, 9),
    		VV VECTOR INITIAL(4, 5, 6),
    		SS SCALAR INITIAL(77),
    		II INTEGER INITIAL(89),
    		AA CHARACTER(10) INITIAL('HELLO'),
    		DD BIT(5) INITIAL(26),
    		BB BOOLEAN;
 
    WRITE(6);
    WRITE(6) 'No-argument FUNCTION tests.';
    IF II = E THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument INTEGER function test', BB) ASSIGN(ERRORS, TOTAL);
    IF SS = F THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument SCALAR function test 1', BB) ASSIGN(ERRORS, TOTAL);
    IF SS = F1 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument SCALAR function test 2', BB) ASSIGN(ERRORS, TOTAL);
    IF SS = F2 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument SCALAR function test 3', BB) ASSIGN(ERRORS, TOTAL);
    IF SS = F3 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument SCALAR function test 4', BB) ASSIGN(ERRORS, TOTAL);
    IF VV = G THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument VECTOR function test', BB) ASSIGN(ERRORS, TOTAL);
    IF MM = H THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument MATRIX function test', BB) ASSIGN(ERRORS, TOTAL);
    /*
    IF AA = C THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument CHARACTER function test', BB) ASSIGN(ERRORS, TOTAL);
    IF DD = B THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('No-argument BIT function test', BB) ASSIGN(ERRORS, TOTAL);
    */

    E: FUNCTION INTEGER; RETURN ROUND(F+12); CLOSE E;
    F: FUNCTION SCALAR; RETURN G . G; CLOSE F;
    F1: FUNCTION SCALAR; RETURN G .G; CLOSE F1;
    F2: FUNCTION SCALAR; RETURN G. G; CLOSE F2;
    F3: FUNCTION SCALAR; RETURN G.G; CLOSE F3;
    G: FUNCTION VECTOR; DECLARE MM MATRIX; MM = H; RETURN MM$(2,*); CLOSE G;
    H: FUNCTION MATRIX; 
       DECLARE MM MATRIX CONSTANT(1, 2, 3, 4, 5, 6, 7, 8, 9);
       RETURN MM;
    CLOSE H;
    C: FUNCTION CHARACTER(10); RETURN 'HELLO'; CLOSE C;
    B: FUNCTION BIT(5); RETURN 26; CLOSE B;
 CLOSE NF_TEST;
 CALL NF_TEST ASSIGN(ERRORS, TOTAL);
 
 /*--------------------------------------------------------------------------*/
 ARRAY_FUNCTIONS: PROCEDURE ASSIGN(ERRORS, TOTAL);
    DECLARE ERRORS INTEGER, TOTAL INTEGER;
    DECLARE A ARRAY(6) INITIAL(1, 2, 3, 4, 5, 6);
    DECLARE BB;
    WRITE(6) ;
    WRITE(6) 'Array functions.';
    IF MIN(A) = 1 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('Array function MIN', BB) ASSIGN(ERRORS, TOTAL);
    IF MAX(A) = 6 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('Array function MAX', BB) ASSIGN(ERRORS, TOTAL);
    IF SUM(A) = 21 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('Array function SUM', BB) ASSIGN(ERRORS, TOTAL);
    IF PROD(A) = 720 THEN BB = TRUE; ELSE BB = FALSE;
    CALL PRINT_RESULT('Array function PROD', BB) ASSIGN(ERRORS, TOTAL);
 CLOSE ARRAY_FUNCTIONS;
 CALL ARRAY_FUNCTIONS ASSIGN(ERRORS, TOTAL);
 
 /*--------------------------------------------------------------------------*/
 ARRAYED_RTL: PROCEDURE ASSIGN(ERRORS, TOTAL);
    DECLARE ERRORS INTEGER, TOTAL INTEGER;
    DECLARE ARRAY(3,2), A, A1, A2, A3, B2, AB BOOLEAN;
    DECLARE BB BOOLEAN, I INTEGER, J INTEGER;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
    	A1$(I,J) = 20 (RANDOM - 0.5);
    	DO UNTIL ROUND(A2$(I,J)) ~= 0; A2$(I,J) = 20 (RANDOM - 0.5); END;
    	B2$(I,J) = 100 (RANDOM + 1);
    	A3$(I,J) = 20 (RANDOM - 0.5);
    END; END;
    WRITE(6) ;
    WRITE(6) 'Arrayed RTL functions.';
    A = ABS(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= ABS(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed ABS', BB) ASSIGN(ERRORS, TOTAL);
    A = CEILING(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= CEILING(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed CEILING', BB) ASSIGN(ERRORS, TOTAL);
    A = DIV(A1, A2);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= DIV(A1$(I,J), A2$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed DIV', BB) ASSIGN(ERRORS, TOTAL);
    A = FLOOR(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= FLOOR(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed FLOOR', BB) ASSIGN(ERRORS, TOTAL);
    A = MIDVAL(A1, A2, A3);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= MIDVAL(A1$(I,J), A2$(I,J), A3$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed MIDVAL', BB) ASSIGN(ERRORS, TOTAL);
    A = MOD(A1, A2);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= MOD(A1$(I,J), A2$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed MOD', BB) ASSIGN(ERRORS, TOTAL);
    AB = ODD(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF AB$(I,J) ~= ODD(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed ODD', BB) ASSIGN(ERRORS, TOTAL);
    A = REMAINDER(A1, A2);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= REMAINDER(A1$(I,J), A2$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed REMAINDER', BB) ASSIGN(ERRORS, TOTAL);
    A = ROUND(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= ROUND(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed ROUND', BB) ASSIGN(ERRORS, TOTAL);
    A = SIGN(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= SIGN(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed SIGN', BB) ASSIGN(ERRORS, TOTAL);
    A = SIGNUM(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= SIGNUM(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed SIGNUM', BB) ASSIGN(ERRORS, TOTAL);
    A = TRUNCATE(A1);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= TRUNCATE(A1$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed TRUNCATE', BB) ASSIGN(ERRORS, TOTAL);
    A = ARCCOS(A1/10);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= ARCCOS(A1$(I,J)/10) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed ARCCOS', BB) ASSIGN(ERRORS, TOTAL);
    A = ARCCOSH(B2);
    BB = TRUE;
    DO FOR I = 1 TO 3; DO FOR J = 1 TO 2;
       IF A$(I,J) ~= ARCCOSH(B2$(I,J)) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed ARCCOSH', BB) ASSIGN(ERRORS, TOTAL);
    WRITE(6) 'More RTL functions could be tested for arrayness, but since';
    WRITE(6) 'they''re all implemented using the same framework, and since';
    WRITE(6) 'we''ve already tested single-argument, double-argument, and';
    WRITE(6) 'triple-argument functions, it seems kind of pointless to';
    WRITE(6) 'continue doing so.  More of these tests can be implemented';
    WRITE(6) 'later if the need arises.';
    
 CLOSE ARRAYED_RTL;
 CALL ARRAYED_RTL ASSIGN(ERRORS, TOTAL);
 
 /*--------------------------------------------------------------------------*/
 WRITE(6);
 WRITE(6) 'Dot-products A.B vs structure references A.B.';
  /* Explanation:  In the final WRITE statement, we should see 14, which is the
    dot product of vector (1,2,3) with itself.  But if the preprocessor doesn't
    correctly mangle the leading G (in G . G), we'll instead see an emulation
    error. */
 DOTPRODUCT_VS_STRUCTURE: FUNCTION; 
 DECLARE G FUNCTION VECTOR;
 F: FUNCTION SCALAR; RETURN G.G; CLOSE F;
 G: FUNCTION VECTOR; DECLARE V VECTOR INITIAL(1,2,3); RETURN V; CLOSE G;
 RETURN F;
 CLOSE DOTPRODUCT_VS_STRUCTURE; 
 IF DOTPRODUCT_VS_STRUCTURE = 14 THEN BB = TRUE; ELSE BB = FALSE;
 CALL PRINT_RESULT('Dot-product vs structure-reference', BB) ASSIGN (ERRORS, TOTAL);
 
 /*--------------------------------------------------------------------------*/	
 WRITE(6) ;
 IF ALLTESTS THEN DO;
    TEMPORARY STIME, ETIME; 
    WRITE(6) 'Not a pass/fail test. Informational execution-speed timing measurement.'; 
    STIME = RUNTIME; 
    M = 100000; 
    I = 0; DO WHILE I < M; I = I + 1; END; 
    ETIME = RUNTIME; 
    WRITE(6) M, 'iterations,', ETIME-STIME, 'seconds, ', 
 	  ROUND(M/(ETIME-STIME)), 'iterations/second.'; 
 END;
 ELSE WRITE(6) 'Skipping informational test(s).';
 	
 /*--------------------------------------------------------------------------*/	
 WRITE(6); 
 WRITE(6) 'Total errors:', ERRORS;
 WRITE(6) 'Total tests:', TOTAL;
 
