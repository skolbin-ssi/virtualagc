       IF AI3$(I,J) ~= AI1$(I,J) - AI2$(I,J) THEN BB = FALSE;
    END; END;
    CALL PRINT_RESULT('Arrayed subtraction of INTEGER', BB) ASSIGN(ERRORS, TOTAL);
