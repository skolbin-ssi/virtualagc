 /*--------------------------------------------------------------------------*/	
  /* Test equality with a tolerance. */
  REALLY_CLOSE_TO: FUNCTION(X, Y) BOOLEAN;
     DECLARE X SCALAR, Y SCALAR;
     DECLARE EPSILON SCALAR CONSTANT(1E-9);
     IF ABS(X - Y) < EPSILON THEN RETURN TRUE; ELSE RETURN FALSE;
  CLOSE REALLY_CLOSE_TO;
 
 /* Convert degrees to radians and vice-versa. */
 DECLARE PI CONSTANT(3.141592653589793);
 DECLARE DEGREES_PER_RADIAN CONSTANT(180 / PI);
 DEGREES_TO_RADIANS: FUNCTION(DEGREES) SCALAR;
    DECLARE SCALAR, DEGREES;
    RETURN DEGREES / DEGREES_PER_RADIAN;
 CLOSE DEGREES_TO_RADIANS;
 RADIANS_TO_DEGREES: FUNCTION(RADIANS) SCALAR;
    DECLARE SCALAR, RADIANS;
    RETURN RADIANS DEGREES_PER_RADIAN;
 CLOSE RADIANS_TO_DEGREES;
 
 RTL_TESTS: PROCEDURE ASSIGN(ERRORS, TOTAL);
    DECLARE INTEGER, ERRORS, TOTAL;
    DECLARE BOOLEAN, BB, BB2;
    DECLARE COSINE_ANGLES VECTOR(17) CONSTANT(0, 30, 45, 60, 90, 120, 135, 150, 
 				180, 210, 225, 240, 270, 300, 315, 330, 360),
            COSINE_VALUES VECTOR(17) CONSTANT(1, SQRT(3)/2, SQRT(2)/2, 1/2, 0, 
         			-1/2, -SQRT(2)/2, -SQRT(3)/2, -1, 
         			-SQRT(3)/2, -SQRT(2)/2, -1/2, 0,
         			1/2, SQRT(2)/2, SQRT(3)/2, 1),
            ARCCOS_VALUES VECTOR(6) CONSTANT(1.5707963267948966 ,
         	1.369438406004566 ,1.1592794807274085 ,0.9272952180016121 ,
         	0.6435011087932843 ,0.0),
            ARCCOSH_VALUES VECTOR(4) CONSTANT(1.3169578969248166 , 
         	1.762747174039086 , 2.0634370688955608 , 2.2924316695611777),
            ARCSIN_VALUES VECTOR(6) CONSTANT(0.0 , 0.2013579207903308 , 
         	0.41151684606748806 , 0.6435011087932845 , 0.9272952180016123 , 
         	1.5707963267948966),
            ARCSINH_VALUES VECTOR(11) CONSTANT(-2.3124383412727525 , 
         	-2.0947125472611012 , -1.8184464592320668 , -1.4436354751788103, 
         	-0.881373587019543 , 0.0 , 0.881373587019543 , 
         	1.4436354751788103 , 1.8184464592320668 , 2.0947125472611012 , 
         	2.3124383412727525),
            ARCTAN2_VALUES MATRIX(5, 5) CONSTANT(-2.356194490192345 , 
         	-2.0344439357957027 , -1.5707963267948966 , -1.1071487177940904, 
         	-0.7853981633974483 , -2.677945044588987 , -2.356194490192345 , 
         	-1.5707963267948966 , -0.7853981633974483 , -0.4636476090008061, 
         	3.141592653589793 , 3.141592653589793 , 0.0 , 0.0, 0.0 , 
         	2.677945044588987 , 2.356194490192345 , 1.5707963267948966 , 
         	0.7853981633974483 , 0.4636476090008061 , 2.356194490192345 , 
         	2.0344439357957027 , 1.5707963267948966 , 1.1071487177940904 , 
         	0.7853981633974483),
            ARCTAN_VALUES VECTOR(11) CONSTANT(-1.373400766945016 , 
         	-1.3258176636680326 , -1.2490457723982544 , -1.1071487177940904, 
         	-0.7853981633974483 , 0.0 , 0.7853981633974483 , 
         	1.1071487177940904 , 1.2490457723982544 , 1.3258176636680326 , 
         	1.373400766945016),
            ARCTANH_VALUES VECTOR(11) CONSTANT(-1.1989476363991853 , 
         	-0.8047189562170501 , -0.5493061443340548 , 
         	-0.34657359027997264 , -0.16823611831060645 , 0.0 , 
         	0.16823611831060645 , 0.34657359027997264 , 0.5493061443340548 , 
         	0.8047189562170501 , 1.1989476363991853),
            COSH_VALUES VECTOR(11) CONSTANT(74.20994852478785 , 27.308232836016487, 
         	10.067661995777765 , 3.7621956910836314 , 1.5430806348152437 , 
         	1.0 , 1.5430806348152437 , 3.7621956910836314 , 
         	10.067661995777765 , 27.308232836016487 , 74.20994852478785),
            SINH_VALUES VECTOR(11) CONSTANT(-74.20321057778875, -27.28991719712775, 
         	-10.017874927409903 , -3.626860407847019 , -1.1752011936438014 , 
         	0.0 , 1.1752011936438014 , 3.626860407847019 , 
         	10.017874927409903 , 27.28991719712775 , 74.20321057778875),
            EXP_VALUES VECTOR(11) CONSTANT(0.006737946999085467 , 
         	0.01831563888873418 , 0.049787068367863944 , 0.1353352832366127, 
         	0.36787944117144233 , 1.0 , 2.718281828459045 , 
         	7.38905609893065 , 20.085536923187668 , 54.598150033144236 , 
         	148.4131591025766),
            SQRT_VALUES VECTOR(11) CONSTANT(0.0 , 1.0 , 1.4142135623730951 , 
         	1.7320508075688772 , 2.0 , 2.23606797749979 , 2.449489742783178, 
         	2.6457513110645907 , 2.8284271247461903 , 3.0 , 
         	3.1622776601683795),
            TAN_VALUES VECTOR(11) CONSTANT(3.380515006246586 , -1.1578212823495775, 
         	0.1425465430742778 , 2.185039863261519 , -1.5574077246549023 , 
         	0.0 , 1.5574077246549023 , -2.185039863261519 , 
         	-0.1425465430742778 , 1.1578212823495775 , -3.380515006246586),
            TANH_VALUES VECTOR(11) CONSTANT(-0.9999092042625951 , 
         	-0.999329299739067 , -0.9950547536867305 , -0.9640275800758169 , 
         	-0.7615941559557649 , 0.0 , 0.7615941559557649 , 
         	0.9640275800758169 , 0.9950547536867305 , 0.999329299739067 , 
         	0.9999092042625951);
    DECLARE V VECTOR(5), V2 VECTOR(5), SUMM SCALAR, M MATRIX(5,5), MT MATRIX(5,5);
    DECLARE B5 BIT(5), B7 BIT(7);
    DECLARE A2 ARRAY(2), A7 ARRAY(7), A12 ARRAY(12), A10V5 ARRAY(10) VECTOR(5);
 
    WRITE(6) ;
    WRITE(6) 'Some unarrayed RTL tests.';
    
    IF ABS(-5.2) = 5.2 AND ABS(0) = 0 AND ABS(153) = 153 THEN BB = TRUE;
    ELSE BB = FALSE;
    CALL PRINT_RESULT('ABS', BB) ASSIGN(ERRORS, TOTAL);
    IF CEILING(-5.2) = -5 AND CEILING(-3.9) = -3 AND CEILING(0.0) = 0
       AND CEILING(1.1) = 2 AND CEILING(5.9) = 6 THEN BB = TRUE;
    ELSE BB = FALSE;
    CALL PRINT_RESULT('CEILING', BB) ASSIGN(ERRORS, TOTAL);
    /* The documentation does not give any direct information about how DIV
       behaves with negative numbers.  If we supposed (which again, the
       documentation does not tell us) that DIV and REMAINDER are mutually
       consistent, then we would expect that for any integers N and M, we'd
       have
        		N = M DIV(N, M) + REMAINDER(N, M)
       The documentation does tell us that 
       		REMAINDER(-5,3) = -2
       		REMAINDER(5, -3) = 2
       		REMAINDER(-5, -3) = -2
       So we can test for that, and test that DIV is consistent with 
       REMAINDER. */ 
    BB = TRUE;
    IF REMAINDER(5,3) ~= 2 OR REMAINDER(-5,3) ~= -2 OR REMAINDER(5,-3) ~= 2 OR
       REMAINDER(-5,-3) ~= -2 THEN DO;
          BB = FALSE;
          WRITE(6) 'REMAINDER ERROR:', REMAINDER(5,3), REMAINDER(-5,3),
             	REMAINDER(5,-3), REMAINDER(-5,-3);
       END;
    DO FOR TEMPORARY N = -10 TO 10; DO FOR TEMPORARY M = -10 TO 10;
       IF M ~= 0 THEN IF N ~= M DIV(N, M) + REMAINDER(N, M) THEN DO;
          BB = FALSE;
          WRITE(6) 'DIV/REMAINDER ERROR:', N, M, DIV(N,M), REMAINDER(N,M);
       END;
    END; END;
    CALL PRINT_RESULT('DIV and REMAINDER', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF FLOOR(-2.6) ~= -3 OR FLOOR(-2.2) ~= -3 OR FLOOR(-3.0) ~= -3 OR
       FLOOR(0) ~= 0 OR FLOOR(2.0) ~= 2 OR FLOOR(2.2) ~= 2 OR FLOOR(2.99) ~= 2
       THEN BB = FALSE;
    CALL PRINT_RESULT('FLOOR', BB) ASSIGN(ERRORS, TOTAL);
    IF MIDVAL(1,1,1) ~= 1 OR MIDVAL(1,1,2) ~= 1 OR MIDVAL(1,1,3) ~= 1 OR
       MIDVAL(1,2,1) ~= 1 OR MIDVAL(1,2,2) ~= 2 OR MIDVAL(1,2,3) ~= 2 OR
       MIDVAL(1,3,1) ~= 1 OR MIDVAL(1,3,2) ~= 2 OR MIDVAL(1,3,3) ~= 3 OR
       MIDVAL(2,1,1) ~= 1 OR MIDVAL(2,1,2) ~= 2 OR MIDVAL(2,1,3) ~= 2 OR
       MIDVAL(2,2,1) ~= 2 OR MIDVAL(2,2,2) ~= 2 OR MIDVAL(2,2,3) ~= 2 OR
       MIDVAL(2,3,1) ~= 2 OR MIDVAL(2,3,2) ~= 2 OR MIDVAL(2,3,3) ~= 3 OR
       MIDVAL(3,1,1) ~= 1 OR MIDVAL(3,1,2) ~= 2 OR MIDVAL(3,1,3) ~= 3 OR
       MIDVAL(3,2,1) ~= 2 OR MIDVAL(3,2,2) ~= 2 OR MIDVAL(3,2,3) ~= 3 OR
       MIDVAL(3,3,1) ~= 3 OR MIDVAL(3,3,2) ~= 3 OR MIDVAL(3,3,3) ~= 3 
       THEN BB = FALSE;
    CALL PRINT_RESULT('MIDVAL', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF MOD(5,3) ~= 2 OR MOD(5,-3) ~= 2 OR MOD(-5,3) ~= 1 OR MOD(-5,-3) ~= 1 
    THEN BB = FALSE;
    IF NOT REALLY_CLOSE_TO(MOD(-5,2.1), 1.3) THEN BB = FALSE;
    CALL PRINT_RESULT('MOD', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF NOT ODD(5) OR ODD(4) OR NOT ODD(3) OR ODD(2) OR NOT ODD(1) OR ODD(0) OR
       NOT ODD(-1) OR ODD(-2) OR NOT ODD(-3) OR 
       ODD(1.8) OR NOT ODD(1.2) OR ODD(-1.9) OR NOT ODD(-2.9) THEN BB = FALSE;
    CALL PRINT_RESULT('ODD', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF ROUND(-5.2) ~= -5 OR ROUND(-4.7) ~= -5 OR ROUND(-4.5) ~= -5 OR
          ROUND(-4.2) ~= -4 OR ROUND(-3) ~= -3 OR ROUND(-2.8) ~= -3 OR
          ROUND(-3.5) ~= -4 OR ROUND(-2) ~= -2 OR ROUND (-1) ~= -1 OR
          ROUND(0) ~= 0 OR ROUND(1) ~= 1 OR ROUND(1.2) ~= 1 OR ROUND(1.5) ~= 2 OR
          ROUND(2.2) ~= 2 OR ROUND(2.5) ~= 3 OR ROUND (3.2) ~= 3 THEN BB = FALSE;
    CALL PRINT_RESULT('ROUND', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF SIGN(-1.5) ~= -1 OR SIGN(0) ~= 1 OR SIGN(1.5) ~= 1 THEN BB = FALSE;
    CALL PRINT_RESULT('SIGN', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF SIGNUM(-1.5) ~= -1 OR SIGNUM(0) ~= 0 OR SIGNUM(1.5) ~= 1 THEN BB = FALSE;
    CALL PRINT_RESULT('SIGNUM', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF TRUNCATE(-3.4) ~= -3 OR TRUNCATE(7.8) ~= 7 THEN BB = FALSE;
    CALL PRINT_RESULT('TRUNCATE', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = 1 TO 6;
       IF NOT REALLY_CLOSE_TO(ARCCOS(0.2 (I-1)), ARCCOS_VALUES$I) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCCOS', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = 2 TO 5;
       IF NOT REALLY_CLOSE_TO(ARCCOSH(I), ARCCOSH_VALUES$(I-1)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCCOSH', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = 1 TO 6;
       IF NOT REALLY_CLOSE_TO(ARCSIN(0.2 (I-1)), ARCSIN_VALUES$I) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCSIN', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(ARCSINH(I), ARCSINH_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCSINH', BB) ASSIGN(ERRORS, TOTAL);
 
    BB = TRUE;
    DO FOR TEMPORARY I = -2 TO 2; DO FOR TEMPORARY J = -2 TO 2;
       IF I ~= 0 OR J ~= 0 
       THEN IF NOT REALLY_CLOSE_TO(ARCTAN2(I, J), ARCTAN2_VALUES$(I+3, J+3)) 
       THEN DO;
          BB = FALSE;
          WRITE(6) 'ARCTAN2 problem: ', I, J, 
          		ARCTAN2(I, J), ARCTAN2_VALUES$(I+3, J+3);
       END;
    END; END;
    CALL PRINT_RESULT('ARCTAN2', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(ARCTAN(I), ARCTAN_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCTAN', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(ARCTANH(I/6), ARCTANH_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('ARCTANH', BB) ASSIGN(ERRORS, TOTAL);
       
    BB = TRUE;
    DO FOR TEMPORARY I = 1 TO 17;
       IF NOT REALLY_CLOSE_TO(COS(DEGREES_TO_RADIANS(COSINE_ANGLES$I)), 
                              COSINE_VALUES$I)
       THEN DO; BB = FALSE; EXIT; END;
    END; 
    CALL PRINT_RESULT('COS', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY THETA = 0 TO 360;
        IF NOT REALLY_CLOSE_TO(SIN(DEGREES_TO_RADIANS(THETA)),
        			    COS(DEGREES_TO_RADIANS(THETA-90))) 
        THEN DO;
           BB = FALSE;
           EXIT;
        END;
    END;
    CALL PRINT_RESULT('SIN', BB) ASSIGN(ERRORS, TOTAL);
 
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(COSH(I), COSH_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('COSH', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(SINH(I), SINH_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('SINH', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(EXP(I), EXP_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('EXP', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(I, LOG(EXP(I))) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('LOG', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(SQRT(I+5), SQRT_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('SQRT', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(TAN(I), TAN_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('TAN', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    DO FOR TEMPORARY I = -5 TO 5;
       IF NOT REALLY_CLOSE_TO(TANH(I), TANH_VALUES$(I+6)) THEN BB = FALSE;
    END;
    CALL PRINT_RESULT('TANH', BB) ASSIGN(ERRORS, TOTAL);
 
    BB = TRUE;
    BB2 = TRUE;
    DO FOR TEMPORARY I = 1 TO 10;
    	SUMM = 0;
 	DO FOR TEMPORARY J = 1 TO 5;
 		V$J = RANDOM;
 		SUMM = SUMM + (V$J)**2;
 	END;
 	SUMM = SQRT(SUMM);
 	IF NOT REALLY_CLOSE_TO(ABVAL(V), SUMM) THEN BB = FALSE;
 	IF SUMM ~= 0 THEN DO;
 		V2 = UNIT(V);
 		V = V / SUMM;
 		IF NOT REALLY_CLOSE_TO(0, ABVAL(V - V2)) THEN BB2 = FALSE;
 	END;
    END;
    CALL PRINT_RESULT('ABVAL', BB) ASSIGN(ERRORS, TOTAL);
    CALL PRINT_RESULT('UNIT', BB2) ASSIGN(ERRORS, TOTAL);
    
    BB = TRUE;
    BB2 = TRUE;
    DO FOR TEMPORARY I = 1 TO 10;
       SUMM = 0;
       DO FOR TEMPORARY J = 1 TO 5;
          DO FOR TEMPORARY K = 1 TO 5;
             M$(J,K) = RANDOM;
             MT$(K,J) = M$(J,K);
             IF J = K THEN SUMM = SUMM + M$(J,K);
          END;
       END;
       IF NOT REALLY_CLOSE_TO(SUMM, TRACE(M)) THEN BB = FALSE;
       IF TRANSPOSE(M) ~= MT THEN BB2 = FALSE;
    END;
    CALL PRINT_RESULT('TRACE', BB) ASSIGN(ERRORS, TOTAL);
    CALL PRINT_RESULT('TRANSPOSE', BB2) ASSIGN(ERRORS, TOTAL);
    WRITE(6) 'NOTE: DET, INVERSE, MAX, MIN, PROD, SUM functions tested later.';
    
    BB = TRUE;
    B5 = BIN'0000000000'; B7 = BIN'0000000000';
    IF XOR(B5, B7) ~= BIN'0' THEN BB = FALSE;
    B5 = BIN'0000000000'; B7 = BIN'1111111111';
    IF XOR(B5, B7) ~= BIN'1111111' THEN BB = FALSE;
    B5 = BIN'1111111111'; B7 = BIN'0000000000';
    IF XOR(B5, B7) ~= BIN'0011111' THEN BB = FALSE;
    B5 = BIN'1111111111'; B7 = BIN'1111111111';
    IF XOR(B5, B7) ~= BIN'1100000' THEN BB = FALSE;
    B5 = BIN'1101'; B7 = BIN'1010101';
    IF XOR(B5, B7) ~= BIN'1011000' THEN BB = FALSE;
    CALL PRINT_RESULT('XOR', BB) ASSIGN(ERRORS, TOTAL);
    
    BB = TRUE;
    IF INDEX('I AM A POTATO', 'POT') ~= 8 THEN BB = FALSE;
    IF INDEX('I AM A POTATO', 'SNORKEL') ~= 0 THEN BB = FALSE;
    IF INDEX('I AM A POTATO', 'I') ~= 1 THEN BB = FALSE;
    CALL PRINT_RESULT('INDEX', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF LENGTH('') ~= 0 THEN BB = FALSE;
    IF LENGTH('RONALD') ~= 6 THEN BB = FALSE;
    IF LENGTH('SUPERSTRUCTURE') ~= 14 THEN BB = FALSE;
    CALL PRINT_RESULT('LENGTH', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF LJUST('', 10) ~= '          ' THEN BB = FALSE;
    IF LJUST('RONALD', 10) ~= 'RONALD    ' THEN BB = FALSE;
    IF LJUST('RONALD', 5) ~= 'RONAL' THEN BB = FALSE;
    /* Need also the check that the line above signaled an error, but I haven't
       implemented anything to do with that yet. */
    CALL PRINT_RESULT('LJUST', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF RJUST('', 10) ~= '          ' THEN BB = FALSE;
    IF RJUST('RONALD', 10) ~= '    RONALD' THEN BB = FALSE;
    IF RJUST('RONALD', 5) ~= 'RONAL' THEN BB = FALSE;
    /* Need also the check that the line above signaled an error, but I haven't
       implemented anything to do with that yet. */
    CALL PRINT_RESULT('RJUST', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF TRIM('') ~= '' THEN BB = FALSE;
    IF TRIM('RONALD ') ~= 'RONALD' THEN BB = FALSE;
    IF TRIM('RONALD      ') ~= 'RONALD' THEN BB = FALSE;
    IF TRIM(' RONALD') ~= 'RONALD' THEN BB = FALSE;
    IF TRIM('    RONALD') ~= 'RONALD' THEN BB = FALSE;
    IF TRIM('   RONALD     ') ~= 'RONALD' THEN BB = FALSE;
    CALL PRINT_RESULT('TRIM', BB) ASSIGN(ERRORS, TOTAL);
 
    WRITE(6) 'Note that there is no apparent way to provide an automated';
    WRITE(6) 'check for most time-related RTL functions.  For informational';
    WRITE(6) 'purposes only, here are some current readings for them:';
    WRITE(6) 'CLOCKTIME =', CLOCKTIME, ', DATE =', DATE, ', RUNTIME =', RUNTIME;
    WRITE(6) 'Note that the HAL/S runtime-error system has not yet been';
    WRITE(6) 'implemented, and therefore cannot be tested at present.  Here';
    WRITE(6) 'are the outputs of the error-reporting functions:';
    WRITE(6) 'ERRGRP =', ERRGRP, ', ERRNUM =', ERRNUM;
    
    MONTE_PI: FUNCTION(N) SCALAR;
       DECLARE N INTEGER, HIT INTEGER;
       HIT = 0;
       DO FOR TEMPORARY I = 1 TO N;
     	  IF RANDOM**2 + RANDOM**2 <= 1 THEN HIT = HIT + 1;
       END;
       RETURN ABS(PI - 4 HIT / N);
    CLOSE MONTE_PI;
    BB = FALSE;
    DO FOR TEMPORARY I = 1 TO 20;
       IF MONTE_PI(1000) < 0.1 THEN DO; BB = TRUE; EXIT; END;
    END;
    CALL PRINT_RESULT('RANDOM', BB) ASSIGN(ERRORS, TOTAL);
    
    /* Swiped this from Wikipedia's Python code in the article called
       "Algorithms for calculating variance". */
    SHIFTED_VARIANCE: PROCEDURE(N) ASSIGN(MEAN, VARIANCE);
       DECLARE N INTEGER, MEAN SCALAR, VARIANCE SCALAR;
       DECLARE K SCALAR, EX SCALAR, EX2 SCALAR;
       K = RANDOMG;
       EX, EX2 = 0.0;
       DO FOR TEMPORARY I = 2 TO N;
          TEMPORARY X;
          X = RANDOMG;
          EX = EX + X - K;
          EX2 = EX2 + (X - K)**2;
       END;
       MEAN = EX / N + K;
       VARIANCE = (EX2 - EX**2 / N) / (N-1);
    CLOSE SHIFTED_VARIANCE;
    BB = FALSE;
    DO FOR TEMPORARY I = 1 TO 20;
       TEMPORARY MEAN, VARIANCE;
       CALL SHIFTED_VARIANCE(1000) ASSIGN(MEAN, VARIANCE);
       IF ABS(MEAN) < 0.1 AND ABS(VARIANCE - 1) < 0.1 THEN DO;
          BB = TRUE;
          EXIT;
       END;
    END;
    CALL PRINT_RESULT('RANDOMG', BB) ASSIGN(ERRORS, TOTAL);
    
    BB = TRUE;
    IF SHL(-2, 2) ~= -8 THEN BB = FALSE;
    IF SHL(1, 1) ~= 2 OR SHL(2, 1) ~= 4 OR SHL(1, 2) ~= 4 THEN BB = FALSE;
    CALL PRINT_RESULT('SHL', BB) ASSIGN(ERRORS, TOTAL);
    BB = TRUE;
    IF SHR(-4,2) ~= -1 THEN BB = FALSE;
    IF SHR(8, 1) ~= 4 OR SHR(4, 1) ~= 2 OR SHR(8, 2) ~= 2 THEN BB = FALSE;
    CALL PRINT_RESULT('SHR', BB) ASSIGN(ERRORS, TOTAL);
    
    BB = TRUE;
    IF SIZE(A2) ~= 2 OR SIZE(A7) ~= 7 OR SIZE(A12) ~= 12 OR SIZE(A10V5) ~= 10
    THEN BB = FALSE;
    CALL PRINT_RESULT('SIZE', BB) ASSIGN(ERRORS, TOTAL);
 
 CLOSE RTL_TESTS;
 CALL RTL_TESTS ASSIGN(ERRORS, TOTAL);

