    DECLARE AI1 ARRAY(5) INTEGER INITIAL(1,2,3,4,5), 
    	    AI2 ARRAY(5) INTEGER INITIAL(2,1,3,4,5),
    	    AI3 ARRAY(3,2) INTEGER INITIAL(1,2,3,4,5,6), 
    	    AI4 ARRAY(3,2) INTEGER INITIAL(2,1,3,4,5,6),
